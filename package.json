{
  "name": "tree-sitter-jacy",
  "version": "0.0.3",
  "description": "Tree-Sitter grammar for Jacy",
  "main": "bindings/node",
  "scripts": {
    "build": "tree-sitter generate && node-gyp build",
    "config": "node-gyp configure --release --verbose --python=python2 --target=$(node --version)",
    "dev": "npm run config && npm run build && tree-sitter generate && tree-sitter test",
    "test": "tree-sitter test && tree-sitter parse examples/* --time"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazer-hazer/Jacy-Atom.git"
  },
  "keywords": [
    "tree-sitter",
    "jacy",
    "jacy-lang",
    "programming-language",
    "syntax",
    "parser"
  ],
  "author": "hazer-hazer",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hazer-hazer/Jacy-Atom/issues"
  },
  "homepage": "https://github.com/hazer-hazer/Jacy-Atom#readme",
  "devDependencies": {
    "tree-sitter-cli": "^0.20.0",
    "node-gyp": "8.1.0"
  },
  "dependencies": {
    "nan": "^2.12.1"
  },
  "tree-sitter": [
    {
      "scope": "source.jc",
      "file-types": [
        "jc"
      ],
      "injection-regex": "jc|jacy"
    }
  ]
}
